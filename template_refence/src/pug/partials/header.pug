								a.header__notification.header__notification_new(href="notifications.html")
									.header__subtitle Request to reset security
									.header__date 2021-03-10 20:19:15
								a.header__notification(href="notifications.html")
									.header__subtitle Login attempted from new IP
									.header__date 2021-03-10 20:19:15
								a.header__notification(href="notifications.html")
									.header__subtitle Request to reset security
									.header__date 2021-03-10 20:19:15
							.header__btns
								a.button-small.header__button(href="notifications.html") View all
								button.button-stroke.button-small.header__button Clear all
					a.button-stroke.button-small.header__button(href="wallet-overview.html" class=classWallet) Wallet
					+theme(true)
					.header__item.header__item_user.js-header-item
						button.header__head.js-header-head
							img(src="img/content/avatar-user.jpg" alt="Avatar")
						.header__body.js-header-body
							a.header__el(href="profile-info.html")
								.header__icon
									+i('user')
								.header__details
									.header__title Profile
									.header__content Important account details
							a.header__el(href="referrals.html")
								.header__icon
									+i('share')
								.header__details
									.header__title Referrals
									.header__content Invite your friends and earn rewards
							a.header__el(href="2fa.html")
								.header__icon
									+i('lock')
								.header__details
									.header__title 2FA security
									.header__content Setup 2FA for more security
							a.header__el(href="api-keys.html")
								.header__icon
									+i('cog')
								.header__details
									.header__title Settings
									.header__content View additional settings
							.header__el
								.header__icon
									+i('theme-dark')
								.header__details
									.header__line
										.header__title Dark mode
										+theme
									.header__content Switch dark/light mode
							a.header__el(href="#")
								.header__icon
									+i('exit')
								.header__details
									.header__title Log out
				.header__btns
					a.button-small.header__button(href="sign-up.html") Sign Up
					a.button-stroke.button-small.header__button(href="sign-in.html") Login
				button.header__burger.js-header-burger
