if !header
	header.header.js-header(class=classHeader data-id="#header")
		.header__center.center
			a.header__logo(href="index.html")
				img.header__pic.header__pic_desktop.some-icon(src="img/logo-light.svg" alt="BitCloud")
				img.header__pic.header__pic_desktop.some-icon-dark(src="img/logo-dark.svg" alt="BitCloud")
				img.header__pic.header__pic_mobile(src="img/logo.svg" alt="BitCloud")
			.header__wrapper
				.header__wrap.js-header-wrap
					nav.header__nav
						a.header__item(href="exchange.html" class=classExchange) Exchange
						.header__item.header__item_dropdown.js-header-item
							button.header__head.js-header-head Buy Crypto
								+i('arrow-down')
							.header__body.js-header-body
								a.header__link(href="buy-crypto.html" class=classCreditCard)
									+i('user')
									| Credit card
								a.header__link(href="deposit-fiat.html" class=classBankDeposit)
									+i('image')
									| Bank deposit
						a.header__item(href="market.html" class=classMarket) Market
						a.header__item(href="learn-crypto.html" class=classDiscover) Discover
					.header__btns
						a.button-small.header__button(href="sign-up.html") Sign Up
						a.button-stroke.button-small.header__button(href="sign-in.html") Login
					a.button-stroke.header__button(href="wallet-overview.html" class=classWallet) Wallet
				.header__item.header__item_settings.js-header-item
					button.header__head.js-header-head EN/USD
						+i('arrow-down')
					.header__body.js-header-body
						.header__row
							.header__col
								.header__category Language
								.header__menu
									a.header__language.active(href="#") 
										span.header__flag ðŸ‡ºðŸ‡¸
