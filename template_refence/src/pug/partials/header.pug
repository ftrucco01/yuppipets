										span.header__flag ðŸ‡·ðŸ‡º
										| Russia
							.header__col
								.header__category Currency
								.header__menu
									a.header__currency.active(href="#") USD
									a.header__currency(href="#") RUB
									a.header__currency(href="#") EUR
									a.header__currency(href="#") JPY
									a.header__currency(href="#") BTC
				.header__control
					a.header__activity(href="activity.html" class=classActivity)
						+i('lightning')
					.header__item.header__item_notifications.js-header-item
						button.header__head.js-header-head.active
							+i('bell')
						.header__body.js-header-body
							.header__title Notifications
							.header__list
								a.header__notification.header__notification_new(href="notifications.html")
									.header__subtitle Login attempted from new IP
									.header__date 2021-03-10 20:19:15
								a.header__notification.header__notification_new(href="notifications.html")
									.header__subtitle Request to reset security
